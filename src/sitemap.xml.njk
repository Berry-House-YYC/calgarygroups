---
permalink: /sitemap.xml
layout: false
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{%- for item in collections.all %}
  {%- if item.url and item.url != "/404.html" and item.url != "/sitemap.xml" and item.url != "/robots.txt" %}
  <url>
    <loc>{{ site.url }}{{ item.url }}</loc>
    <lastmod>{{ item.date | default: build.generatedAt | toRfc822 }}</lastmod>
  </url>
  {%- endif %}
{%- endfor %}
</urlset>
